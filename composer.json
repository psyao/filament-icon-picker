{
    "name": "psyao/filament-icon-picker",
    "type": "library",
    "description": "A Filament form field that provides a searchable icon picker backed by Blade UI Icons manifests.",
    "keywords": [
        "psyao",
        "laravel",
        "filament",
        "filament-form",
        "filament-field",
        "icon",
        "icon-picker",
        "blade-icons",
        "blade-ui-kit",
        "filament-icon-picker"
    ],
    "homepage": "https://github.com/psyao/filament-icon-picker",
    "support": {
        "issues": "https://github.com/psyao/filament-icon-picker/issues",
        "source": "https://github.com/psyao/filament-icon-picker",
        "docs": "https://github.com/psyao/filament-icon-picker#readme",
        "changelog": "https://github.com/psyao/filament-icon-picker/releases"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/psyao"
        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Aguet",
            "email": "psyaosan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.6",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "*",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Psyao\\FilamentIconPicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psyao\\FilamentIconPicker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
