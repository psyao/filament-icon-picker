{
    "name": "psyao/filament-icon-picker",
    "description": "Add an icon picker to the filament forms",
    "keywords": [
        "psyao",
        "laravel",
        "filament-icon-picker"
    ],
    "homepage": "https://github.com/psyao/filament-icon-picker",
    "support": {
        "issues": "https://github.com/psyao/filament-icon-picker/issues",
        "source": "https://github.com/psyao/filament-icon-picker"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Aguet",
            "email": "psyaosan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.6",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "*",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Psyao\\IconPicker\\": "src/",
            "Psyao\\IconPicker\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psyao\\IconPicker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
